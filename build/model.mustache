module.exports = request => {

	const {{className}} = {
		name: '{{className}}',
		path: '{{{pathName}}}'
	};

	{{#apis}}
	{{className}}.{{method}} = function() {

		return request({
			modelName:      {{className}}.name,
			methodName:     '{{method}}',
			httpMethod:     '{{httpMethod}}',
			path:           '{{{path}}}',
			params:         [{{#arguments}}'{{{.}}}', {{/arguments}}],
			{{#usesFormData}}usesFormData:   {{usesFormData}},
			{{/usesFormData}}{{#withoutPayload}}withoutPayload: {{withoutPayload}},
			{{/withoutPayload}}args:           Array.prototype.slice.call(arguments)
		});

	};

	{{/apis}}
	return {{className}};

};